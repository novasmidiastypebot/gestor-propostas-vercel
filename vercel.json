{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/efi",
      "dest": "/api/efi"
    },
    {
      "src": "/api/health",
      "dest": "/api/health"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}